#include <stdio.h>

int main() {
   int data[2][2];

   printf("Informe o dia \n");
   scanf("%d", &data[0][0]);
         if(data[0][0] >= 31) {
            printf("O mes tem no maximo 31 dias");
            break;}

   printf("Informe o mes \n");
   scanf("%d", &data[0][1]);
         if(data[0][1] > 12) {
            printf("O ano tem 12 meses \n");
            break;}

  

   
